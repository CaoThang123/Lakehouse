# AWS Region dùng cho Spark/S3 (MinIO giả lập S3)
AWS_REGION=us-east-1

# Phù hợp với MinIO region (mặc định)
MINIO_REGION=us-east-1

# Dùng cho pyIceberg (catalog)
AWS_DEFAULT_REGION=us-east-1

# AWS credentials, dùng để kết nối MinIO
AWS_ACCESS_KEY_ID=minioadmin       # trùng với MinIO ROOT_USER
AWS_SECRET_ACCESS_KEY=minioadmin   # trùng với MinIO ROOT_PASSWORD

# Endpoint MinIO, Spark/Iceberg sẽ connect qua HTTP API
AWS_S3_ENDPOINT=http://minio_server:9000

# Thư mục lưu dữ liệu Iceberg / warehouse path
WAREHOUSE=s3a://warehouse/

# URI của Nessie Catalog (Iceberg versioning)
NESSIE_URI=http://nessie:19120/api/v1
